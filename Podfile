# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'DigitalLocker' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/Firestore'
  # Pods for DigitalLocker

  target 'DigitalLockerTests' do
    inherit! :search_paths
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Database'
    pod 'Firebase/Storage'
    pod 'Firebase/Firestore'
    # Pods for testing
  end

  target 'DigitalLockerUITests' do
    inherit! :search_paths
    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Database'
    pod 'Firebase/Storage'
    pod 'Firebase/Firestore'
    # Pods for testing
  end

end
